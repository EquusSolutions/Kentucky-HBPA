@model IEnumerable<KYHBPA.Models.Event>

@*<div class="body-content-padded">*@
    <data>
        @foreach (var ev in Model)
        {
            <event id="@ev.Id">
                @*<start_date><![CDATA[@String.Format("{0:MM/dd/%y HH:mm}", @ev.StartDate)]]></start_date>*@
                <start_date><![CDATA[@String.Format("{0:MM/dd/yyyy HH:mm}", @ev.StartDate)]]></start_date>
                @*<end_date><![CDATA[@String.Format("{0:MM/dd/%y HH:mm}", @ev.EndDate)]]></end_date>*@
                <end_date><![CDATA[@String.Format("{0:MM/dd/yyyy HH:mm}", @ev.EndDate)]]></end_date>
                <text><![CDATA[@ev.Description]]></text>
            </event>
        }
    </data>
@*</div>*@